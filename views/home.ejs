<%- include('header') -%>
<%- include('navbar') -%>
<main>
  <div class="container h-100 mt-5 pt-5">
    <div class="row g-3">
      <% data.forEach((product)=>{ %>
        <div class="col-lg-3">
          <div class="card">
            <img src="/static/images/<%= product.image %>" class="card-img-top" alt="...">
            <div class="card-body">
              <h5 class="card-title"><%= product.name %> <span class="text-danger fw-bold"> <%=product.price %></span></h5>
              <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
              <a href="#" class="btn btn-primary">Buy Now</a>
            </div>
          </div>
        </div>
        
        <% }) %>
    </div>
   
  </div>
    


</main>




<%- include('footer') -%>
